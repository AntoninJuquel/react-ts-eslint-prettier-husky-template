{
  "name": "react-ts-eslint-prettier-husky-template",
  "version": "1.0.0",
  "description": "React + TypeScript + Vite + Github Actions starter template",
  "repository": {
    "type": "git",
    "url": "https://github.com/AntoninJuquel/react-ts-eslint-prettier-husky-template"
  },
  "keywords": [
    "react",
    "vite",
    "typescript",
    "template",
    "ghactions",
    "vitest"
  ],
  "author": "AntoninJuquel",
  "license": "MIT",
  "homepage": "https://AntoninJuquel.github.io/react-ts-eslint-prettier-husky-template",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:ci": "tsc && vite build && cp 404.html dist/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage",
    "test:cy": "cypress open",
    "serve": "vite preview",
    "prepare": "husky install && rm -rf .git/hooks && ln -s ../.husky .git/hooks",
    "lint": "eslint --cache --fix ./src ./cypress ./stories ./tests ./types",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@commitlint/prompt-cli": "^17.0.3",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/node": "^18.6.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vitejs/plugin-react": "^2.0.0",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "c8": "^7.12.0",
    "cypress": "^10.7.0",
    "daisyui": "^2.27.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.4",
    "eslint-plugin-tailwindcss": "^3.6.1",
    "eslint-plugin-testing-library": "^5.6.0",
    "gh-pages": "^4.0.0",
    "happy-dom": "^6.0.4",
    "husky": "^8.0.1",
    "i18next": "^21.9.1",
    "i18next-browser-languagedetector": "^6.1.5",
    "i18next-http-backend": "^1.4.1",
    "lint-staged": "^11.2.3",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "react-i18next": "^11.18.5",
    "styled-components": "^5.3.5",
    "tailwindcss": "^3.1.8",
    "twin.macro": "^2.8.2",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.21.0"
  }
}
